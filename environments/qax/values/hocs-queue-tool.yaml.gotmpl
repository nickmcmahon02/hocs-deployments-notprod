---
hocs-generic-service:
  version: {{ .Values.versions.hocs-queue-tool }}

  deployment:
    annotations:
      downscaler/downtime: "Mon-Sun 00:00-07:55 Europe/London,Mon-Sun 18:05-24:00 Europe/London"

  truststore:
    resources:
      requests:
        cpu: 50m

  app:
    env:
      queues:
        - 'audit'
        - 'case-creator'
        - 'document'
        - 'notify'
        - 'opensearch'
        - 'search'
    resources:
      requests:
        cpu: 50m
